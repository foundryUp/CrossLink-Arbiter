{
  "name": "chainlink-arbitrage-bot",
  "version": "1.0.0",
  "description": "Cross-Domain Arbitrage Bot with MEV focus using Chainlink CCIP and SUAVE Helios",
  "keywords": [
    "arbitrage",
    "MEV",
    "chainlink",
    "ccip",
    "suave",
    "defi",
    "cross-chain",
    "ethereum",
    "arbitrum",
    "avalanche"
  ],
  "author": "Arbitrage Bot Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/chainlink-arbitrage-bot.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "format": "prettier --write .",
    "deploy:testnet": "node scripts/deploy.js --network testnet",
    "deploy:mainnet": "node scripts/deploy.js --network mainnet",
    "setup:chainlink": "node chainlink/functions/deploy.js && node chainlink/automation/register.js",
    "monitor": "node monitoring/dashboard/app.js",
    "agents": "python -m agents.main",
    "build": "tsc",
    "clean": "rm -rf dist coverage .nyc_output"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.8.0",
    "@openzeppelin/contracts": "^5.0.0",
    "@ethersproject/providers": "^5.7.2",
    "@ethersproject/contracts": "^5.7.0",
    "@ethersproject/wallet": "^5.7.0",
    "ethers": "^6.8.0",
    "web3": "^4.2.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "redis": "^4.6.10",
    "pg": "^8.11.3",
    "sequelize": "^6.34.0",
    "bull": "^4.11.4",
    "socket.io": "^4.7.4",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "node-cron": "^3.0.3",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/express": "^4.17.20",
    "@types/cors": "^2.8.15",
    "@types/compression": "^1.7.4",
    "@types/morgan": "^1.9.7",
    "@types/pg": "^8.10.7",
    "@types/ws": "^8.5.8",
    "@types/jest": "^29.5.6",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.51.0",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.{js,ts}",
      "!src/**/*.test.{js,ts}",
      "!src/**/*.spec.{js,ts}"
    ],
    "testMatch": [
      "**/tests/**/*.test.{js,ts}",
      "**/tests/**/*.spec.{js,ts}"
    ]
  }
} 